  "tokenColors": [
    {
      "scope": [
        "meta.decorator variable.other.constant.property.ts",
        "meta.object.member variable.other.constant.property.ts"
      ],
      "settings": {
        "foreground": "#B6EE65"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#ACE061"
      }
    },
    {
      "scope": [
        "meta.method.declaration meta.definition.method entity.name.function.ts"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.decorator variable.other.object.ts"],
      "settings": {
        "foreground": "#19ffc9"
      }
    },
    {
      "scope": ["meta.decorator meta.function-call entity.name.function"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "meta.object-binding-pattern-variable meta.definition.variable variable.other.constant.ts",
        "meta.parameters meta.decorator variable.other.readwrite.ts",
        "meta.decorator variable.other.readwrite.ts"
      ],
      "settings": {
        "foreground": "#00C2B2"
      }
